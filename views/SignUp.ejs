            <form action="/login" method="post">
                <input type="text" name="username" id="" class="in1" placeholder="username" required><br>
                <input type="text" name="password" id="" class="in2" placeholder="password"><br>
                <input type="text" name="email" id="" class="in3" placeholder="email"><br>
                <input type="text" name="univ" id="" class="in4" placeholder="universitÃ©"><br>
                <input type="text" name="profession" id="" class="in5" placeholder="profession"><br>
